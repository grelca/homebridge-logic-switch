{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true,
    "jest/globals": true
  },
  "extends": "standard-with-typescript",
  "plugins": [
    "@typescript-eslint",
    "jest"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "@typescript-eslint/no-extraneous-class": 0
  }
}
